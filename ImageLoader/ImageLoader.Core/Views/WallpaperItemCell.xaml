<?xml version="1.0" encoding="utf-8" ?>
<ViewCell xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ImageLoader.Core.Views.WallpaperItemCell">
  <Grid>
    <Image Aspect="AspectFill"  Source="{Binding Image}" x:Name="WallpaperImage"/>
    <Label x:Name="WallpaperName" Text="{Binding Name}"/>
  </Grid>
</ViewCell>